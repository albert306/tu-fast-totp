{
    "manifest_version": 2,
    "name": "tu-fast-totp",
    "version": "1.0",
  
    "description": "automates the required totp 2FA process on the TU-Dresden ZIH login ",
  
    "icons": {
      "48": "icons/icon.svg"
    },

    "permissions": ["activeTab", "tabs", "storage"],

    "options_ui": {
      "page": "options/options.html"
    },

    "browser_action": {
      "default_icon": "icons/icon-48.png",
      "default_title": "tu-fast-totp",
      "default_popup": "popup/popup.html"
    }
  }
  